<script type="text/template" class="extended-carousel-template">
	<% _.each( days, function( day ) { %>
		<li role="row">
			<div class="regular-label" role="rowheader"><%= day.day_offset === 0 ? "Today" : day.day_offset === 1 ? "Tomorrow" : day.day %></div>
			<div class="body-medium uppercase" role="cell"><%= day.date_display %></div>
			<i class="condition-icon icon icon-weather-<%= day.icon_name %>" aria-hidden="true" role="cell"></i>
			<div class="body-medium body-sky-text" role="cell"><%= day.sky_text %></div>
			<div class="high" role="cell">
				<% if ( day.day_offset > 0 || moment().tz(timezone).format("H") < 15 ) { %>
					<span class="body-medium">High</span> <span class="regular-label color-black"><%= day.high_f %>&deg;F</span>
				<% } %>
			</div>
			<div class="low" role="cell"><span class="body-medium">Low</span> <span class="regular-label color-black"><%= day.low_f %>&deg;F</span></div>
			<div class="body-medium" role="cell">Chance of<br/>precip</div>
			<div class="regular-label rain" role="cell"><i class="icon icon-rain-drop" aria-hidden="true"></i> <%= day.precip_chance %>%</div>
		</li>
	<% } ) %>
</script>

<script type="text/template" class="text-carousel-template">
	<% _.each( days, function( day ) { %>
		<% if ( day.desc_day.length !== 0 ) { %>
		<li role="group" aria-label="forecast">
			<div class="regular-label"><%= day.day_offset === 0 ? "Today" : day.day_offset === 1 ? "Tomorrow" : day.day %></div>
			<div class="body-text"><%= day.desc_day %></div>
		</li>
		<% } %>
		<% if ( day.desc_night.length !== 0 ) { %>
		<li role="group" aria-label="forecast">
			<div class="regular-label"><%= day.day_offset === 0 ? "Tonight" : day.day_offset === 1 ? "Tomorrow Night" : day.day + " Night" %></div>
			<div class="body-text"><%= day.desc_night %></div>
		</li>
		<% } %>
	<% } ) %>
</script>
