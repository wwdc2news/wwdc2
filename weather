<script type="text/template" class="weather-lead-template">
	<div class="weather-page-detail--current-temp">
		<div class="row">
			<span class="weather-page-detail--current-temp-value"><%= data.current.temp_f %></span>
			<span class="weather-page-detail--current-temp-deg">&deg;</span>
			<span class="weather-page-detail--current-temp-f">F</span>
			<i class="weather-page-detail--current-icon icon icon-weather-<%= data.current.icon_name %>" aria-hidden="true"></i>
		</div>
	</div>

	<div class="weather-page-detail--current-conditions row">
		<div class="c1">Feels like <span class="regular-label"><%= data.current.feels_like_f %>&deg;F</span></div>
		<div class="c1"><span class="sr-only"> <%= data.current.sky %> </span> <%= data.current.sky %></div>
		<div class="c2 regular-label"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.hourly[0].precip_chance %>%</div>
	</div>

	<div class="weather-page-detail--condition-list" role="table" tabindex="0">
		<% if ( moment().tz(data.tz).format("H") < 15 ) { %>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Today</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[0].sky_text_day %> </span><i class="icon icon-weather-<%= data.daily[0].icon_name_day %>" aria-hidden="true"></i> <%= data.daily[0].high_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span><i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.daily[0].precip_chance_day %>%</div>
			</div>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Tonight</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[0].sky_text_night %> </span><i class="icon icon-weather-<%= data.daily[0].icon_name_night %>" aria-hidden="true"></i> <%= data.daily[0].low_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.daily[0].precip_chance_night %>%</div>
			</div>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Tomorrow</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[1].sky_text_day %> </span><i class="icon icon-weather-<%= data.daily[1].icon_name_day %>" aria-hidden="true"></i> <%= data.daily[1].high_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i><%= data.daily[1].precip_chance_day %>%</div>
			</div>
		<% } else { %>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Tonight</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[0].sky_text_night %>  </span><i class="icon icon-weather-<%= data.daily[0].icon_name_night %>" aria-hidden="true"></i> <%= data.daily[0].low_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.daily[0].precip_chance_night %>%</div>
			</div>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Tomorrow</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[1].sky_text_day %> </span><i class="icon icon-weather-<%= data.daily[1].icon_name_day %>" aria-hidden="true"></i> <%= data.daily[1].high_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.daily[1].precip_chance_day %>%</div>
			</div>
			<div class="row" role="row">
				<div class="c1 body-medium" role="rowheader">Tomorrow Night</div>
				<div class="c1 regular-label" role="cell"><span class="sr-only"> <%= data.daily[1].sky_text_night %> </span><i class="icon icon-weather-<%= data.daily[1].icon_name_night %>" aria-hidden="true"></i> <%= data.daily[1].low_f %>&deg;F</div>
				<div class="c2 regular-label" role="cell"><span class="sr-only"> Chance of Precipitation </span> <i class="icon icon-rain-drop" aria-hidden="true"></i> <%= data.daily[1].precip_chance_night %>%</div>
			</div>
		<% } %>
		<%
			var transform = function( value ) {
				var matches = /([1-2]?[0-9]:[0-9]{2}):[0-9]{2} (am|pm)/.exec( value );
				return matches.length === 3 ? matches[ 1 ] + " " + matches[ 2 ] : "";
			}

			var sunrise = transform( data.daily[0].sunrise );
			var sunset = transform( data.daily[0].sunset );
		%>
		<div class="row" role="row">
			<div class="c1 body-medium" role="rowheader">Wind</div>
			<div class="c2 regular-label" role="cell"><i class="icon icon-weather-wind" aria-hidden="true"></i> <%= data.current.wind_dir_card %> <%= data.current.wind_speed_mph %> mph</div>
		</div>
		<div class="row" role="row">
			<div class="c1 body-medium" role="rowheader">Humidity</div>
			<div class="c2 regular-label" role="cell"><i class="icon icon-weather-stats_humidity" aria-hidden="true"></i> <%= data.current.rel_humidity %>%</div>
		</div>
		<div class="row" role="row">
			<div class="c1 body-medium" role="rowheader">Sunrise</div>
			<div class="c2 regular-label uppercase" role="cell"><i class="icon icon-weather-sunrise" aria-hidden="true"></i> <%= sunrise %></div>
		</div>
		<div class="row" role="row">
			<span class="c1 body-medium" role="rowheader">Sunset</span>
			<div class="c2 regular-label uppercase" role="cell"><i class="icon icon-weather-sunset" aria-hidden="true"></i> <%= sunset %></div>
		</div>
		<div class="row" role="row">
			<span class="c1 body-medium" role="rowheader">Record High</span>
			<div class="c2 regular-label" role="cell"><i class="icon icon-weather-stats_rec_high" aria-hidden="true"></i> <%= data.historical.record_high_f %>&deg;F</div>
		</div>
		<div class="row" role="row">
			<span class="c1 body-medium" role="rowheader">Record Low</span>
			<div class="c2 regular-label" role="cell" aria-colspan="2"><i class="icon icon-weather-stats_rec_low" aria-hidden="true"></i> <%= data.historical.record_low_f %>&deg;F</div>
		</div>
	</div>

	<div class="show-more-less">
		<span class="small-label uppercase">Show<br>More</span>
		<i class="fa fa-chevron-down" aria-hidden="true"></i>
	</div>
</script>

