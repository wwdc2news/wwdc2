<script type="text/template" class="weekend-forecast-template">
	<% _.each( days, function( day ) { %>
		<% if ( day.desc_day.length !== 0 ) { %>
			<li role="group" aria-label="forecast">
				<i class="icon icon-weather-<%= day.icon_name_day %>" aria-hidden="true"></i>
				<div class="text">
					<div class="regular-label"><%= day.day_offset === 0 ? "Today" : day.day_offset === 1 ? "Tomorrow" : day.day %></div>
					<div class="body-medium"><%= day.desc_day %></div>
				</div>
			</li>
		<% } %>
		<% if ( day.desc_night.length !== 0 ) { %>
			<li role="group" aria-label="forecast">
				<i class="icon icon-weather-<%= day.icon_name_night %>" aria-hidden="true"></i>
				<div class="text">
					<div class="regular-label"><%= day.day_offset === 0 ? "Tonight" : day.day_offset === 1 ? "Tomorrow Night" : day.day + " Night" %></div>
					<div class="body-medium"><%= day.desc_night %></div>
				</div>
			</li>
		<% } %>
	<% } ) %>
</script>
<!-- / listing-page/items/grid-weather_weekend -->

        
                                            
                        
                        
                        
                        
